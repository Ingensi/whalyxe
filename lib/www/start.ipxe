#!ipxe

set menu-timeout 0
:start
menu ~ Ingensi Whalyxe ~
item --gap      Operating systems :
item ubuntu		Ubuntu
item centos		CentOS
item coreos     CoreOS
item rancheros  RancherOS
item --gap      Actions :
item netinfo	Network information
item reload		Reload
item reboot		Reboot
choose selected || goto start
goto sub_file

:sub_file
chain ${selected}.ipxe
chain start.ipxe